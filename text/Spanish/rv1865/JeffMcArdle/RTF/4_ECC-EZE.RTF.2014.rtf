\pard\plain \s4\nowidctlpar\f56\fs20\qj\fi60\li0\ri0\sb0\sa0\faauto\sl200\slmult0\expnd0\expndtw0\cf0\charscalex0\b0\i0\ul0\strike0\accnone\hyphpar1\lang2058
