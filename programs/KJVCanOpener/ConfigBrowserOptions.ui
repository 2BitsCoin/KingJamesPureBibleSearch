 <class>CConfigBrowserOptions</class>
 <widget class="QWidget" name="CConfigBrowserOptions">
  <property name="windowTitle">
   <string>Browser Options</string>
  </property>
  <layout class="QGridLayout" name="gridLayout">
   <item row="0" column="0">
    <layout class="QHBoxLayout" name="horizontalLayout">
     <item>
      <widget class="QLabel" name="lblBrowserNavigationActivationDelay">
       <property name="text">
        <string>Navigation Activation &amp;Delay (ms):</string>
       </property>
       <property name="buddy">
        <cstring>spinBrowserNavigationActivationDelay</cstring>
       </property>
      </widget>
     </item>
     <item>
      <widget class="CSpinBox" name="spinBrowserNavigationActivationDelay">
       <property name="minimum">
        <number>50</number>
       </property>
       <property name="maximum">
        <number>2000</number>
       </property>
       <property name="singleStep">
        <number>50</number>
       </property>
       <property name="value">
        <number>500</number>
       </property>
      </widget>
     </item>
    </layout>
   </item>
   <item row="1" column="0">
    <layout class="QHBoxLayout" name="horizontalLayout_2">
     <item>
      <widget class="QLabel" name="lblBrowserPassageReferenceActivationDelay">
       <property name="text">
        <string>Passage Reference Activation &amp;Delay (ms):</string>
       </property>
       <property name="buddy">
        <cstring>spinBrowserPassageReferenceActivationDelay</cstring>
       </property>
      </widget>
     </item>
     <item>
      <widget class="CSpinBox" name="spinBrowserPassageReferenceActivationDelay">
       <property name="minimum">
        <number>100</number>
       </property>
       <property name="maximum">
        <number>4000</number>
       </property>
       <property name="singleStep">
        <number>100</number>
       </property>
       <property name="value">
        <number>1000</number>
       </property>
      </widget>
     </item>
    </layout>
   </item>
   <item row="2" column="0">
    <widget class="QCheckBox" name="checkBoxShowExcludedSearchResults">
     <property name="text">
      <string>Show E&amp;xcluded Search Results</string>
     </property>
    </widget>
   </item>
   <item row="3" column="0">
    <layout class="QHBoxLayout" name="horizontalLayout_3">
     <item>
      <widget class="QLabel" name="lblChapterScrollbarMode">
       <property name="text">
        <string>Entire Bible &amp;Scrollbar:</string>
       </property>
       <property name="buddy">
        <cstring>comboBoxChapterScrollbarMode</cstring>
       </property>
      </widget>
     </item>
     <item>
      <widget class="QComboBox" name="comboBoxChapterScrollbarMode">
       <property name="sizePolicy">
        <sizepolicy hsizetype="Preferred" vsizetype="Fixed">
         <horstretch>10</horstretch>
         <verstretch>0</verstretch>
        </sizepolicy>
       </property>
      </widget>
     </item>
    </layout>
   </item>
   <item row="4" column="0">
    <layout class="QHBoxLayout" name="horizontalLayout_4">
     <item>
      <widget class="QLabel" name="lblVerseRenderingMode">
       <property name="text">
        <string>Verse &amp;Rendering:</string>
       </property>
       <property name="buddy">
        <cstring>comboBoxVerseRenderingMode</cstring>
       </property>
      </widget>
     </item>
     <item>
      <widget class="QComboBox" name="comboBoxVerseRenderingMode">
       <property name="sizePolicy">
        <sizepolicy hsizetype="Preferred" vsizetype="Fixed">
         <horstretch>10</horstretch>
         <verstretch>0</verstretch>
        </sizepolicy>
       </property>
      </widget>
     </item>
    </layout>
   </item>
   <item row="5" column="0">
    <widget class="QCheckBox" name="checkBoxShowPilcrowMarkers">
     <property name="text">
      <string>Show &amp;Pilcrow (Â¶) Symbols (Paragraph Marker)</string>
     </property>
    </widget>
   </item>
  </layout>
 </widget>
 <customwidgets>
  <customwidget>
   <class>CSpinBox</class>
   <extends>QSpinBox</extends>
   <header>SubControls.h</header>
  </customwidget>
 </customwidgets>
 <tabstops>
  <tabstop>spinBrowserNavigationActivationDelay</tabstop>
  <tabstop>spinBrowserPassageReferenceActivationDelay</tabstop>
  <tabstop>checkBoxShowExcludedSearchResults</tabstop>
  <tabstop>comboBoxChapterScrollbarMode</tabstop>
  <tabstop>comboBoxVerseRenderingMode</tabstop>
 </tabstops>
